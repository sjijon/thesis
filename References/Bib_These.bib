@article{Antona2013,
	abstract = {Although few measles cases were reported in France during 2006 and 2007,$\backslash$nsuggesting the country might have been close to eliminating the disease,$\backslash$na dramatic outbreak of {\textgreater}20,000 cases occurred during 2008-2011.$\backslash$nAdolescents and young adults accounted for more than half of cases;$\backslash$nmedian patient age increased from 12 to 16 years during the outbreak.$\backslash$nThe highest incidence rate was observed in children {\textless}1 year of age,$\backslash$nreaching 135 cases/100,000 infants during the last epidemic wave. Almost$\backslash$n5,000 patients were hospitalized, including 1,023 for severe pneumonia$\backslash$nand 27 for encephalitis/myelitis; 10 patients died. More than 80{\%} of$\backslash$nthe cases during this period occurred in unvaccinated persons,$\backslash$nreflecting heterogeneous vaccination coverage, where pockets of$\backslash$nsusceptible persons still remain. Although vaccine coverage among$\backslash$nchildren improved, convincing susceptible young adults to get vaccinated$\backslash$nremains a critical issue if the target to eliminate the disease by 2015$\backslash$nis to be met.},
